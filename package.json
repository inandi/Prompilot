{
  "name": "prompilot",
  "displayName": "Prompilot",
  "description": "Manage custom AI instructions with a status bar button",
  "version": "1.0.2",
  "publisher": "iNandi",
  "icon": "media/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/inandi/prompilot.git"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "prompilot.showPrompts",
        "title": "Show Prompts"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}